<zimlet name="com_cmaginet_phishing"
        version="0.0.1"
        target="main view-window"
        label="ownCloud"
        description="Attach from and save to ownCloud">

  <handlerObject>com_cmaginet_phishing_HandlerObject</handlerObject>
  <serverExtension hasKeyword="url" extensionClass="com.zimbra.cs.zimlet.handler.RegexHandler"/>
  <contentObject type="url">
    <matchOn>
      <regex attrs="ig">user|person</regex>
    </matchOn>
    <toolTip>Hello World!</toolTip>
  </contentObject>
</zimlet>
