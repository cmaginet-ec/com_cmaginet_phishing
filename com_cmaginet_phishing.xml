<zimlet
  name="com_cmaginet_phishing"
  version="0.0.6"
  target="main view-window"
  label="${msg.zimletLabel}"
  description="${msg.description}">

  <include>cmaginet_urls.js</include>
  <handlerObject>com_cmaginet_phishing_handler</handlerObject>

  <contentObject>
    <matchOn>
      <regex attrs="ig">user|person</regex> <!-- matches on the words user or person -->
    </matchOn>
    <toolTip>Hello World!</toolTip>
  </contentObject>
</zimlet>
